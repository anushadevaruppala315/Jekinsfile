pipeline {

   agent any 
    
   stages {
    
	 stage("compile stage") {
	   
	    steps {
		    withMaven(maven : 'maven_3_8_6') {
			   sh 'mvn clean compile'
			  }
	    }
	   
	 }
      
	  stage ("testing stage") {
	  
	    steps {
		     withMaven(maven : 'maven_3_8_6') {
			    sh 'mvn test'
	       }
	    } 
	  }
	 
	 stage ("deployment stage") {
	  
	    steps {
		    withMaven(maven : 'maven_3_8_6') {
			   sh 'mvn delpoy'
		    }
		  }
		}
}
